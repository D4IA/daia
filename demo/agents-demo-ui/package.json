{
  "name": "@d4ia/agents-demo-ui",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "format:fix": "prettier ./src --write",
    "format:check": "prettier ./src --check",
    "format:check:cached": "prettier ./src --check --cache",
    "lint": "eslint ./src --max-warnings 0",
    "lint:check": "eslint ./src --max-warnings 0",
    "lint:cached": "eslint ./src --max-warnings 0 --cache",
    "lint:fix": "eslint ./src --fix",
    "check:cached": "npm run lint:check && npm run format:check:cached",
    "test:mutate": "stryker run",
    "test": "vitest",
    "test:ss": "vitest -u",
    "build:check": "tsc --noEmit --project tsconfig.app.json",
    "fix": "npm run format:fix && npm run lint:fix",
    "pre-commit-check": "npm run build:check && npm run check:cached",
    "pre-push-check": "npm run build:check && npm run check:cached && npm run test",
    "prepublishOnly": "npm run build && npm run lint && npm run format:check && npm run test",
    "dev": "node scripts/generate-config.js && vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006 --ci",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@d4ia/agents-demos": "workspace:*",
    "@d4ia/blockchain-bridge": "workspace:*",
    "@d4ia/core": "workspace:*",
    "postcss": "~8.5.4",
    "postcss-preset-mantine": "~1.17.0",
    "postcss-simple-vars": "~7.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "~7.11.0",
    "sass-embedded": "~1.89.1",
    "vite-plugin-pwa": "~1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@storybook/react": "~9.1.3",
    "@storybook/react-vite": "~9.1.3",
    "@stryker-mutator/vitest-runner": "~9.0.1",
    "@testing-library/dom": "~10.4.0",
    "@testing-library/jest-dom": "~6.6.3",
    "@testing-library/react": "~16.3.0",
    "@testing-library/user-event": "~14.6.1",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@vitest/coverage-istanbul": "~3.2.4",
    "autoprefixer": "^10.4.19",
    "daisyui": "^5.3.10",
    "dotenv": "^17.2.3",
    "eslint": "^9.26.0",
    "eslint-config-mantine": "~4.0.3",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rimraf": "~6.0.1",
    "sass-embedded": "~1.89.1",
    "storybook": "~9.1.3",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite": "^6.3.5",
    "vitest": "3.2.4"
  }
}
