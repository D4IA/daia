{
  "name": "@d4ia/agents-demos",
  "version": "0.0.2-SNAPSHOT.8",
  "description": "DAIA protocol utilities",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/agents-demos.es.js",
      "require": "./dist/agents-demos.cjs.js"
    }
  },
  "main": "./dist/agents-demos.cjs.js",
  "module": "./dist/agents-demos.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "private": true,
  "scripts": {
    "build": "rimraf dist && vite build",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test-watch": "vitest",
    "start": "tsx src/index.ts",
    "demo:enter": "tsx src/enterDemo.ts",
    "demo:exit": "tsx src/exitDemo.ts",
    "genkeys": "tsx src/genKeys.ts",
    "format:fix": "prettier ./src --write",
    "format:check": "prettier ./src --check",
    "format:check:cached": "prettier ./src --check --cache",
    "lint": "eslint ./src --max-warnings 0",
    "lint:check": "eslint ./src --max-warnings 0",
    "lint:cached": "eslint ./src --max-warnings 0 --cache",
    "lint:fix": "eslint ./src --fix",
    "check:cached": "npm run lint:check && npm run format:check:cached",
    "build:check": "tsc --noEmit",
    "fix": "npm run format:fix && npm run lint:fix"
  },
  "author": "teawithsand",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/coverage-istanbul": "~3.2.4",
    "eslint": "^9.26.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "3.2.4",
    "rimraf": "~6.0.1",
    "jsdom": "~27.2.0",
    "zod": "~4.1.13",
    "@bsv/sdk": "~1.9.29",
    "tsx": "~4.21.0",
    "dotenv": "~17.2.3",
    "@types/node": "^24.8.1",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "zod": "~4.1.13"
  },
  "dependencies": {
    "@d4ia/blockchain-bridge": "workspace:*",
    "@d4ia/core": "workspace:*",
    "@d4ia/langchain": "workspace:*",
    "@langchain/core": "~1.1.7",
    "@langchain/langgraph": "~1.0.7",
    "@langchain/openai": "~1.2.0",
    "express": "~5.2.1",
    "immer": "~11.1.3"
  }
}
